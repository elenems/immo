{
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
    "react/jsx-props-no-spreading": ["error", {
      "html": "ignore",
      "custom": "ignore",
      "exceptions": [""]
    }],
    "react/forbid-prop-types": ["off"],
    "jsx-a11y/anchor-is-valid": ["off"]
  },
  "env": {
    "jest": true
}
}